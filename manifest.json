{
  "manifest_version": 2,
  "name": "Cache Hit",
  "description": "Shows if cache was HIT or MISS for the current page/resource.",
  "author":"Jerry Price",  
  "version": "1.1.1",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html",
    "default_title": "checking...."
  },
  "background": {
    "scripts": ["bg.js"]
  },
  "permissions": [
    "webRequest", 
    "webNavigation", 
    "http://*/*", 
    "https://*/*"
  ]
}